#include "hspcl32.as"
	HCLini 1
	if HCLDevCount=0:dialog "対応ビデオカードがありません。終了します",1:end
	mes "OpenCL対応デバイス数"+HCLDevCount+"\n\n\n"

	repeat HCLDevCount
	HCLSetDev cnt
		mes "デバイス名＝  "+HCLGetDeviceInfo_s(CL_DEVICE_NAME);デバイス名取得
		mes "デバイスがOpenCLのdouble演算を使えるか  "+HCLGetDevfp64flg();デバイスがOpenCLのdouble演算を使えるか
		mes "\n"
	loop
	HCLbye
	stop