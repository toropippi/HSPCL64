#include "hspcl32.as"
	HCLini 1
	if HCLDevCount=0:dialog "対応ビデオカードがありません。終了します",1:end
	mes "OpenCL対応デバイス数"+HCLDevCount+"\n\n\n"

	repeat HCLDevCount
	HCLSetDev cnt
		mes "デバイス名＝  "+HCLGetDeviceInfo_s(CL_DEVICE_NAME);デバイス名取得
		mes "OpenCLとOpenGLの連携    "+HCLGetDevGLflg();デバイスがOpenCLとOpenGLの連携を使えるか、つかえるなら１つかえないなら０
		mes "\n"
	loop
	HCLbye
	stop