#include "hspcl32.as"
	HCLini 1

	b=234
	c=4567.89901

	fdim a,7;float型配列変数の作成
	a.0=float(123)//float代入
	a.1=float(b)//float変換
	a.2=float(c)//float変換
	a.3++//インクリメント
	a.4=a.0*a.1//乗算も使えるよ
	a.5=float(sqrt(double(a.4)))//平方根するにはわざわざdoubleに戻さないといけない

	repeat 7
		mes a.cnt
	loop

	mes "\n"
	mes "vartype="+vartype(a)+""
	stop